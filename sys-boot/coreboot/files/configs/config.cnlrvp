CONFIG_VENDOR_INTEL=y
CONFIG_BOARD_INTEL_CANNONLAKE_RVPY=y
CONFIG_CBFS_SIZE=0x300000

CONFIG_PAYLOAD_NONE=y
CONFIG_RESET_ON_INVALID_RAMSTAGE_CACHE=y
CONFIG_SPI_FLASH=y
CONFIG_SPI_FLASH_SMM=y
CONFIG_USE_BLOBS=y
CONFIG_GBB_FLAG_DEV_SCREEN_SHORT_DELAY=y
CONFIG_GBB_FLAG_FORCE_DEV_SWITCH_ON=y
CONFIG_VBOOT_FWID_VERSION="$(KERNELVERSION)"
CONFIG_DISPLAY_FSP_CALLS_AND_STATUS=y

# CONFIG_CONSOLE_SERIAL is not set

# Chrome OS
CONFIG_CHROMEOS=y
CONFIG_SEPARATE_VERSTAGE=y
CONFIG_VBOOT_STARTS_IN_BOOTBLOCK=y

# Event Logging
CONFIG_CMOS_POST=y
CONFIG_CMOS_POST_OFFSET=0x70
CONFIG_COLLECT_TIMESTAMPS=y
CONFIG_ELOG=y
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144

# Firmware Support Package
CONFIG_ADD_FSP_BINARIES=y
CONFIG_HAVE_INTEL_FSP_REPO=y
# CONFIG_FSP_USE_REPO is not set
CONFIG_FSP_M_FILE="3rdparty/blobs/mainboard/intel/cnlrvp/fsp/fspm.bin"
CONFIG_FSP_S_FILE="3rdparty/blobs/mainboard/intel/cnlrvp/fsp/fsps.bin"
CONFIG_FSP_HEADER_PATH="3rdparty/blobs/mainboard/intel/cnlrvp/fsp/Include/"
CONFIG_RUN_FSP_GOP=y
CONFIG_INTEL_GMA_ADD_VBT=y
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/intel/cnlrvp/vbt.bin"

# SPI Descriptor
CONFIG_HAVE_IFD_BIN=y
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/intel/cnlrvp/descriptor.bin"

# Management Engine
CONFIG_HAVE_ME_BIN=y
CONFIG_ME_BIN_PATH="3rdparty/blobs/mainboard/intel/cnlrvp/me.bin"

# Microcode
CONFIG_CPU_MICROCODE_CBFS_EXTERNAL_BINS=y
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/intel/cnlrvp/ucode.bin"

# EC
CONFIG_HAVE_EC_BIN=y
CONFIG_EC_BIN_PATH="3rdparty/blobs/mainboard/intel/cnlrvp/ec.bin"
